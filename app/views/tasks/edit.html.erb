<div class="container">
  <div>
    <%= form_for(@task) do |f| %>
      <%= f.label :title, "Enter a title" %>
      <%= f.text_field :title%>
  </div>
  <div>
    <%= f.label :details, "Enter details for the task" %>
    <%= f.text_field :details %>
  </div>
  <div>
    <%= f.label :completed %>
    <%= f.check_box :completed %>
    <%= f.submit  %>
  <% end %>
  </div>
</div>




